algoritmo "semnome"
var
   espacos: vetor[1..10] de Caractere
   cont, reserva:inteiro
inicio
   Para cont <- 1 ate 10 faca
      Escreva("[ B ",cont," ]")
   FimPara
   Escreval("")
   Escreva("----------------------------")
   Escreval("")
   Escreval("Reservar a cadeira")
   Leia(reserva)
fimalgoritmo